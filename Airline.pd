class Airline ^=
abstract
	var 
		name : string,
		aircraft : set of Aircraft,
		pilots : set of Pilot,
		schedule : set of Flight;
interface
	schema !addFlight(flight : Flight)
		pre flight ~in schedule
		post schedule! = schedule.append(flight)
		
	build{}
		post ?;
end;

// End
